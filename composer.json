{
    "name": "tnb/laravel-installer",
    "description": "Laravel web installer",
    "license": "MIT",
    "authors": [
        {
            "name": "Rachid Laasri",
            "email": "rashidlaasri@gmail.com"
        },
        {
            "name": "Jeremy Kenedy",
            "email": "jeremykenedy@gmail.com"
        },
        {
            "name": "耗子",
            "email": "i@haozi.net"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "TNB\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "TNB\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
